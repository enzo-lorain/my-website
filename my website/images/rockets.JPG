    <style id="globalStyles" type="text/css">/* makes it look OK on initial mobile render */
@media(max-width: 639px) {
  #gen-nav-commerce-header {
    min-height: 48px;
  }
}

/* when a modal is open lock page scrolling */
body[data-active-dialog] {
  overflow-y: hidden;
  position: relative;
}

.browse-dialog [role="dialog"] {
  background: white;
}

.browse-dialog__panel {
  max-height: calc(100vh - 40px);
}

.browse-dialog [role="dialog"] > div {
  background: white;
}

.nikeID-modal-container > div {
  display: 'flex';
  opacity: '1';
  width: '100%';
  height: '100%';
}

/* used to hack in the cookie banner */
#gen-nav-commerce-header .l-header {
  position: relative;
}

@media (min-width: 1024px)  {
  /* also used to hack in the cookie banner */
  .header-wrapper {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1;
  }
}

/* used to lock scrolling for the nav + app when any overlay is active */
.overlay-active {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}

/* used to prevent the branch.io banner from overlaying the mobile navigation */
@media(max-width: 639px) {
  #branch-banner-iframe {
      box-shadow: none !important;
      z-index: auto !important;
    }
}
</style>
